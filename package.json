{
  "name": "nso",
  "version": "0.3.0",
  "description": "NPM Space Observatory",
  "main": "src/www/main.ts",
  "homepage": "https://travis-ci.org/gregoryruiz/nso",
  "scripts": {
    "build:prod": "webpack --config config/webpack.config.prod.js",
    "build:test": "tsc -p test",
    "build": "run-s clean build:prod",
    "clean": "del dist",
    "serve:prod": "webpack-dev-server webpack/hot/only-dev-server --inline --content-base dist/",
    "serve:test": "run-p watch:build-test watch:test",
    "serve": "webpack-dev-server --config config/webpack.config.dev.js",
    "start": "npm run serve",
    "test:unit": "ava",
    "test": "run-s build:test test:unit",
    "watch:build-test": "tsc -p test -w",
    "watch:test": "ava -w"
  },
  "author": "Douglas Duteil",
  "contributors": [
    "Gr√©gory Ruiz <gregory.ruiz@gmail.com> (https://github.com/gregoryruiz)"
  ],
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.4",
    "angular-ui-router": "^1.0.0-rc.1",
    "babel-polyfill": "^6.23.0",
    "core-js": "^2.4.1",
    "d3": "^4.8.0",
    "lodash-es": "^4.17.4",
    "vis": "^4.19.1"
  },
  "ava": {
    "babel": "inherit",
    "concurrency": 5,
    "require": [
      "babel-register",
      "babel-polyfill",
      "jsdom-global/register",
      "./test/_register-src-path.js",
      "./test/_register-babel.js"
    ],
    "files": [
      "out-tsc/**/*.spec.js"
    ],
    "sources": [
      "out-tsc/**/*.js"
    ]
  },
  "devDependencies": {
    "@types/angular": "*",
    "@types/chai": "*",
    "@types/d3": "*",
    "@types/isomorphic-fetch": "*",
    "@types/lodash-es": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "@types/sinon": "*",
    "@types/vis": "*",
    "app-module-path": "^2.2.0",
    "ava": "^0.19.1",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.1",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.3.3",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "del-cli": "^1.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^10.0.0",
    "jsdom-global": "^3.0.0",
    "karma": "^1.6.0",
    "karma-jsdom-launcher": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.17.0",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
